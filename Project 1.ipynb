def LookAt(pl,nw):
    hashes = []
    for post in pl:
        postWords = []
        #removes the common words
        for word in post.lower().split():
            if word not in nw:
                postWords.append(word)
        #removes hashtags
        for word in postWords:
            if word[0] == '#':
                hashes.append(word)
                postWords.remove(word)
        #removes emoji/non-ascii
        for word in postWords:
            for ch in word:
                if ch > "\x7F":
                    postWords.remove(word)
                    break
        #removes links
        for word in postWords:
            if 'http' in word or 'bit.ly' in word:
                postWords.remove(word)
        print(postWords)
